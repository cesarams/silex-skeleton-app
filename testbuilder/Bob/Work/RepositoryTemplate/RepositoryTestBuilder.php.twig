{% extends "base/common.php.twig" %}

{% block functions %}
    /**
     * insert
     *
     * @param array $params parameters passed to rest api
     *
     * @return mixed
     */
    public function insert($params)
    {

        $params['updated_at'] = date("Y-m-d H:i:s");

        return $this->db->insert($this->table, $params);
    }
{% endblock %}